{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/backend-transporte/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/backend-transporte/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/backend-transporte",
  "projects": {
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "backend-transporte": {
      "type": "application",
      "root": "apps/backend-transporte",
      "entryFile": "main",
      "sourceRoot": "apps/backend-transporte/src",
      "compilerOptions": {
        "tsConfigPath": "apps/backend-transporte/tsconfig.app.json"
      }
    },
    "report-service": {
      "type": "application",
      "root": "apps/report-service",
      "entryFile": "main",
      "sourceRoot": "apps/report-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/report-service/tsconfig.app.json"
      }
    },
    "route-service": {
      "type": "application",
      "root": "apps/route-service",
      "entryFile": "main",
      "sourceRoot": "apps/route-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/route-service/tsconfig.app.json"
      }
    }
  }
}